{
	microschema(name: "vcard") {
		name
		uuid
	}
	role(name: "admin") {
		name
		uuid
	}
	tagFamily(name: "colors") { 
		uuid, 
		name, 
		creator { firstname, lastname }
		tags(page: 1, perPage: 1) {
			elements {
				name
			}
		}
	}
	schema(name: "content") { 
		uuid
		name
	}
	content(path:"/Neuigkeiten/News Overview.de.html") {
		node {
			# [ $.data.content.node.uuid=<is-uuid>]
			uuid,
			languagePaths(linkType: FULL) {
				languageTag, 
				link
			}
			content(language: "en") {
				# [ $.data.content.node.content.language=en]
				language
			}
			availableLanguages, 
			project { 
				name, 
				rootNode { uuid }
			}
			created,
			creator {
				username, 
				groups(page:1, perPage:1) {
					elements { 
						name
						roles { 
							elements { name } 
						}
					}
				}
			}
		}
	}
}