		# [$.data.node.availableLanguages[0]=en]
		availableLanguages
		container(language: "en") {
			fields {
				... on content { title },
				... on folder { 
					# [$.data.node.container.fields.name=2015]
					name

					nodeRef {
						# [$.data.node.container.fields.nodeRef.uuid=<is-uuid>]
						uuid

						# [$.data.node.container.fields.nodeRef.isContainer=false]
						isContainer

