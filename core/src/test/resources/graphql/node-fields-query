							# [$.data.node.container.fields.nodeRef.container.isPublished=true]
							isPublished

							# [$.data.node.container.fields.nodeRef.container.isDraft=false]
							isDraft

							fields {
							# [$.data.node.container.fields.nodeRef.container.fields.title=News Overview english title]
							... on content { title }
							... on folder { name }
							}
						}
					},
					binary {
