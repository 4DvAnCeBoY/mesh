<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.3.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd"
         name="cailun">

    <version position="none"/>
    <publishDate position="none"/>

    <bannerLeft>
        <src>cailun_small.png</src>
    </bannerLeft>

    <skin>
        <groupId>lt.velykis.maven.skins</groupId>
        <artifactId>reflow-maven-skin</artifactId>
        <version>1.1.1</version>
    </skin>

    <custom>
        <reflowSkin>
            <theme>bootswatch-spacelab</theme>
            <highlightJs>true</highlightJs>
            <brand>
                <name>CaiLun</name>
                <href>index.html</href>
            </brand>
            <slogan>TBD</slogan>
            <toc>sidebar</toc>
            <topNav>Intro|Documentation|Github</topNav>
            <bottomNav maxSpan="12">
                <column>Intro</column>
                <column>Project Info</column>
                <column>Documentation</column>
                <column>Downloads|downloads</column>
            </bottomNav>
            <pages>
                <maven>
                    <toc>false</toc>
                </maven>
                <index project="neo4vertx-doc">
                    <shortTitle>Welcome</shortTitle>
                    <breadcrumbs>false</breadcrumbs>
                    <toc>false</toc>
                    <sections>
                        <body/>
                        <sidebar/>
                        <thumbs>2</thumbs>
                        <columns>3</columns>
                    </sections>
                </index>
            </pages>
        </reflowSkin>
    </custom>

    <body>
        <head>
            <link rel="shortcut icon" href="favicon.gif" type="image/x-icon"/>
        </head>

        <menu name="Intro">
            <item name="Home" href="index.html"></item>
        </menu>

    </body>
</project>
